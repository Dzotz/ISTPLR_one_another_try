@model Query

@{
    ViewData["Title"] = "ComplexQuery1";
}

<h1>Ускладнений запит 1</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                Назва           
            </th>
            <th>
                Адреса
            </th>

        </tr>
    </thead>
    <tbody>
        @for (int i = 0; i < Model.CoffeeShopNames.Count; i++)
        {
            <tr>
                <td>
                    @Model.CoffeeShopNames[i]
                </td>
                <td>
                    @Model.CoffeeShopAddresses[i]
                </td>

            </tr>
        }
    </tbody>
</table>